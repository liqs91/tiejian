{
  "version": 2,
  "buildCommand": "NODE_OPTIONS=--openssl-legacy-provider npm run build",
  "outputDirectory": "pitch",
  "installCommand": "npm install --legacy-peer-deps",
  "rewrites": [
    {
      "source": "/service-api/(.*)",
      "destination": "http://110.40.176.144:8001/service-api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/pitch/index.html"
    }
  ],
  "headers": [
    {
      "source": "/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ],
  "comment": "触发重新部署以重置权限设置"
}
