{
  "version": 2,
  "builds": [
    { "src": "package.json", "use": "@vercel/static" }
  ],
  "routes": [
    { "handle": "filesystem" },
    { "src": "/static/(.*)", "dest": "/pitch/static/$1" },
    { "src": "/favicon.ico", "dest": "/pitch/favicon.ico" },
    { "src": "/favicon.svg", "dest": "/pitch/favicon.svg" },
    { "src": "/robots.txt", "dest": "/pitch/robots.txt" },
    { "src": "/(.*\\.js)$", "dest": "/pitch/$1" },
    { "src": "/(.*\\.css)$", "dest": "/pitch/$1" },
    { "src": "/(.*\\.(png|jpg|jpeg|gif|svg|ico|woff|woff2|ttf|eot))$", "dest": "/pitch/$1" },
    { "src": "/.*", "dest": "/pitch/index.html" },
    { "src": "/(.*)", "dest": "/pitch/$1" }
  ]
}
