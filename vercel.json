{
  "version": 2,
  "buildCommand": "echo 'Using pre-built files'",
  "outputDirectory": "pitch",
  "installCommand": "echo 'No install needed'",
  "framework": null,
  "rewrites": [
    {
      "source": "/manifest.json",
      "destination": "/pitch/manifest.json"
    },
    {
      "source": "/favicon.ico",
      "destination": "/pitch/favicon.ico"
    },
    {
      "source": "/static/(.*)",
      "destination": "/pitch/static/$1"
    },
    {
      "source": "/pitch/(.*)",
      "destination": "/pitch/$1"
    },
    {
      "source": "/",
      "destination": "/pitch/index.html"
    },
    {
      "source": "/(.*)",
      "destination": "/pitch/index.html"
    }
  ]
}
